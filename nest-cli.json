{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/consistent-bets/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/consistent-bets/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/consistent-bets",
  "projects": {
    "consistent-bets": {
      "type": "application",
      "root": "apps/consistent-bets",
      "entryFile": "main",
      "sourceRoot": "apps/consistent-bets/src",
      "compilerOptions": {
        "tsConfigPath": "apps/consistent-bets/tsconfig.app.json"
      }
    },
    "transactions-worker": {
      "type": "application",
      "root": "apps/transactions-worker",
      "entryFile": "main",
      "sourceRoot": "apps/transactions-worker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/transactions-worker/tsconfig.app.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    }
  }
}